{
  "name": "vrv",
  "description": "A Vite + React + TypeScript template for VSCode",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=24.0.0"
  },
  "volta": {
    "node": "24.12.0"
  },
  "codeQuality": {
    "eslint": "all",
    "typescript": true
  },
  "scripts": {
    "check": "tsc -b --noEmit && eslint .",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^24",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.3.1"
  }
}
