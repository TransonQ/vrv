{
  "name": "vrv",
  "description": "A Vite + React + TypeScript template for VSCode",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "packageManager": "pnpm@10.25.0",
  "engines": {
    "node": ">=22.0.0"
  },
  "codeQuality": {
    "eslint": "all",
    "typescript": true
  },
  "scripts": {
    "check": "tsc -b --noEmit && eslint .",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.25",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.3.0"
  }
}